  <Alpha mId="AlphaHFExprDaily" id="hzheng.xxx.expression001" uId="Ashare" delay="0" exprId="hzheng.xxx.expression001">
    <AlphaInfo author="hzheng" category="Reversion_momentum"/>
    <Config>
      <Expr name="prev_gap" expression="data(open,1)/data(cumadjfactor,1)-data(close,2)/data(cumadjfactor,2)"/>
      <Expr name="mid_strength" expression="(data(curr_close_matrix_143000,1)/data(cumadjfactor,1)-data(curr_close_matrix_103000,1)/data(cumadjfactor,1))/(abs(data(curr_close_matrix_103000,1)/data(cumadjfactor,1))+0.0001)"/>
      <Expr name="seg_volume" expression="(data(curr_volume_matrix_140000,1)-data(curr_volume_matrix_103000,1))/data(cumsplit,1)"/>
      <Expr name="vol_ratio" expression="seg_volume/(tsmean(seg_volume,20)+0.0001)"/>
      <Expr name="hzheng.xxx.expression001" expression="${PROC} -zscore(prev_gap) + 0.8*zscore(mid_strength) -0.5*zscore(vol_ratio) ${PROC2}"/>
    </Config>
    <Op mId="OpDecay" days="5"/>
    <Op mId="OpNeutralize" groups="market" factors="returns20:1 returns5:1" rankMode="1"/>
    <Op mId="OpIndNeut" group="GROUPHSZZ2"/>
  </Alpha>

