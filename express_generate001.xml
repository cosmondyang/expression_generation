      <Expr name="vol_skew" expression="tsmean(vol_avg,5)/(tsmean(vol_avg,20)+0.0001)-1"/>
      <Expr name="hzheng.xxx.expression004" expression="${PROC} -zscore(close_pos)-0.6*zscore(vol_skew)+tsrank(adj_low,15) ${PROC2}"/>
